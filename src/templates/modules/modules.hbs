<div class="mmooc-modules">
    <h1>Kursmoduler</h1>
    {{#each modules}}
        <div class="mmooc-module">
            <h2>{{name}}</h2>
            <a href="{{urlForFirstNoneCompleteItem items}}" class="btn rounded {{#ifAllItemsCompleted items}} btn-done {{/ifAllItemsCompleted}}">GÃ¥ til modul</a>
            {{#if items}}
                <ul class="mmooc-module-items-icons">
                    {{#each items}}
                        <li class="mmooc-module-item-icon"><a class="{{#ifItemIsCompleted completion_requirement}}mmooc-module-item-done{{/ifItemIsCompleted}}" href="{{html_url}}">IKON</a></li>
                    {{/each}}
                </ul>
            {{/if}}
        </div>
    {{/each}}
</div>
